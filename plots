<body>
  <div id="map" style='width:700px;height:500px'></div>
  <script>
 var map = L.map('map').setView([7.577700, 8.993351], 18);
mapLink = '<a href="http://openstreetmap.org">openstreetmap</a>,'+'<a href="http://mapsnigeriainitiative.wordpress.com">mapsNI</a>,'+'Census data:NPC 2006'; 
L.tileLayer('http://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {attribution: ' &copy; ' + mapLink,maxZoom: 22}).addTo(map);

var style= {fillColor:'brown', color:'black', fillOpacity:0.5};

  $.getJSON("https://github.com/mayotunde/work2/blob/gh-pages/LandPlots.json", function(data) {
    var geojson = L.geoJson(data).addTo(map);
  });
  </script>
