  <script>
 var map = L.map('map').setView([4.60, 2.90], 4);
mapLink = '<a href="http://openstreetmap.org">openstreetmap</a>,'+'<a href="http://mapsnigeriainitiative.wordpress.com">mapsNI</a>,'+'Census data:NPC 2006'; 
L.tileLayer(
'https://api.tiles.mapbox.com/v4/mapbox.streets/{z}/{x}/{y}.png?access_token=pk.eyJ1IjoibWFwYm94IiwiYSI6IjZjNmRjNzk3ZmE2MTcwOTEwMGY0MzU3YjUzOWFmNWZhIn0.Y8bhBaUMqFiPrDRW9hieoQ',
{ attribution:' Map data &copy;' + mapLink, maxZoom: 18, }).addTo(map);

var style= {fillColor:'FED976', color:'black', fillOpacity:0.5};

  $.getJSON("https://cdn.rawgit.com/mayotunde/work2/gh-pages/SWRegionMap.json", function(data) {
    var geojson = L.geoJson(data, style).addTo(map);
      
  });
  </script>
